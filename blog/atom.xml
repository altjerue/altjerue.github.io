---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title> Astroclimber Blog </title>
    <link href="http://altjerue.github.io/blog/atom.xml" rel="self"/>
    <link href="http://altjerue.github.io/blog"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://altjerue.github.io/blog</id>
    <author>
	<name>Jesus Rueda-Becerril</name>
	<email>jesus.mrb103@gmail.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
	<title>{{ post.title }}</title>
	<link href="http://altjerue.github.io{{ post.url }}"/>
	<updated>{{ post.date | date_to_xmlschema }}</updated>
	<id>http://altjerue.github.io{{ post.id }}</id>
	<content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>
